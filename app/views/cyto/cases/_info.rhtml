<% a_case = info %>

<%= render :partial => '/cyto/cases/case_info_frame', :object => a_case %>
<%= render :partial => '/patients/compact_frame', :object => a_case.patient %>
<fieldset style="width: 100%; margin-top: 10px">
  <legend>Arzt</legend>
  <strong><%= link_to a_case.doctor.praxis.full_name, :controller => '/doctors', :action => :show, :id => a_case.doctor %>, <%=h a_case.doctor.praxis.locality %></strong>
</fieldset>
<%= render :partial => '/cyto/cases/compact_list', :object => a_case.patient.cases unless a_case.patient.nil? %>

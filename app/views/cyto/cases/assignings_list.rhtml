<h1>Anschreiblisten</h1>

<table>
  <th>Zugewiesen um</th>
  <th>Anzahl</th>
  <th>Tagesnummern</th>
  <th>Eingangsnummern</th>

<% for assigning in @assignings %>
  <tr>
    <td><%= assigning.assigned_at %></td>
    <td><%= assigning.count %></td>
    <td><%= assigning.min_intra_day_id %> - <%= assigning.max_intra_day_id %></td>
    <td><%= assigning.min_praxistar_eingangsnr %> - <%= assigning.max_praxistar_eingangsnr %></td>
  </tr>
<% end %>
</table>

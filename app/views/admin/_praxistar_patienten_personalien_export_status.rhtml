<table class="listing">
  <tr>
    <th>Gestartet um</th>
    <th>Anzahl</th>
    <th>Mutiert</th>
    <th>Neu angelegt</th>
  </tr>
  <% for export in Praxistar::Exports.find(:all, :conditions => "model = 'Praxistar::PatientenPersonalien'", :order => 'finished_at DESC', :limit => 10) %>
    <tr>
      <td><%=h export.started_at %></td>
      <td><%=h export.record_count %></td>
      <td><%=h export.update_count %></td>
      <td><%=h export.create_count %></td>
    </tr>
  <% end %>
</table>

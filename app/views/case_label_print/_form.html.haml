.modal
  = modal_header 'ObjekttrÃ¤ger Etiketten drucken'

  .modal-body{:style => 'height: 150px'}
    = simple_form_for 'case_label', :remote => true do |f|
      .row-fluid
        .span12
          = f.input 'start_praxistar_eingangsnr', :collection => label_select, :selected => @first_case_code, :input_html => {:class => 'select2'}
          = f.input 'end_praxistar_eingangsnr', :collection => label_select, :selected => @last_case_code, :input_html => {:class => 'select2'}
      .row-fluid.hidden-actions
        .form-actions
          = f.button :submit

  .modal-footer
    = link_to t_action(:print), '#', :class => 'btn submit-button'
    = link_to t_action(:cancel), '#', :class => 'btn', 'data-dismiss' => 'modal'
